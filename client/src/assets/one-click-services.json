[{
    "name": "Mongo",
    "description": "MongoDB document databases provide high availability and easy scalability.",
    "website": "https://www.mongodb.com/",
    "image": {
        "name": "mongo",
        "tag": "latest"
    },
    "environment": {
        "MONGO_INITDB_ROOT_USERNAME": "root",
        "MONGO_INITDB_ROOT_PASSWORD": "example"
    }
}, {
