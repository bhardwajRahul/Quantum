[{
    "name": "ownCloud",
    "description": "An open-source file sync, share and content collaboration software that lets teams work on data easily from anywhere, on any device.",
    "website": "https://owncloud.com/",
    "image": {
        "name": "owncloud/server",
        "tag": "latest"
    },
    "ports": [{
        "protocol": "tcp",
        "internalPort": 8080
    }],
    "environment": {
        "OWNCLOUD_TRUSTED_DOMAINS": "{server_ip}",
        "OWNCLOUD_ADMIN_USERNAME": "admin",
        "OWNCLOUD_ADMIN_PASSWORD": "toortoor"
    },
    "volumes": [{
        "containerPath": "/mnt/data"
    }]
}, {
    "name": "Mongo",
    "description": "MongoDB document databases provide high availability and easy scalability.",
    "website": "https://www.mongodb.com/",
    "image": {
        "name": "mongo",
        "tag": "latest"
    },
    "notification": {
        "title": "Your Mongo Deployment",
        "message": "Successfully deployed. The default credentials are username: \"root\" and password: \"example\". You can update these credentials by modifying the environment variables as needed."
    },
    "ports": [{
        "protocol": "tcp",
        "internalPort": 27017
    }],
    "volumes": [{
        "containerPath": "/data/db",
        "mode": "rw"
    }],
    "environment": {
        "MONGO_INITDB_ROOT_USERNAME": "root",
        "MONGO_INITDB_ROOT_PASSWORD": "example"
    }
}, {
    "name": "PostgreSQL",
    "description": "The PostgreSQL object-relational database system provides reliability and data integrity.",
    "website": "https://www.postgresql.org/",
    "image": {
        "name": "postgres",
        "tag": "latest"
    },
    "volumes": [{
        "containerPath": "/var/lib/postgresql/data",
        "mode": "rw"
    }],
    "ports": [{
        "protocol": "tcp",
        "internalPort": 5432
    }],
    "environment": {
        "POSTGRES_PASSWORD": "example"
    }
}, {
    "name": "MySQL",
    "description": "MySQL is a widely used, open-source relational database management system (RDBMS).",
    "website": "https://mysql.com/",
    "image": {
        "name": "mysql",
        "tag": "latest"
    },
    "volumes": [{
        "containerPath": "/var/lib/mysql",
        "mode": "rw"
    }],
    "ports": [{
        "protocol": "tcp",
        "internalPort": 3306
    }],
    "environment": {
        "MYSQL_ROOT_PASSWORD": "my-secret-pw",
        "MYSQL_ALLOW_EMPTY_PASSWORD": "yes",
        "MYSQL_RANDOM_ROOT_PASSWORD": "yes"
    }
}, {
    "name": "Kali Linux",
    "description": "Penetration Testing and Ethical Hacking Linux Distribution.",
    "website": "https://www.kali.org/",
    "command": "/bin/sh",
    "image": {
        "name": "kalilinux/kali-rolling",
        "tag": "latest"
    }
}, {
    "name": "Alpine Linux",
    "description": "Alpine Linux is a security-oriented, lightweight Linux distribution based on musl libc and busybox.",
    "website": "https://alpinelinux.org",
    "command": "/bin/sh",
    "image": {
        "name": "alpine",
        "tag": "latest"
    }
}, {
    "name": "Ubuntu",
    "description": "Ubuntu is a Debian-based Linux operating system based on free software.",
    "website": "https://ubuntu.com",
    "command": "/bin/sh",
    "image": {
        "name": "ubuntu",
        "tag": "latest"
    }
},{
    "name": "Code Server",
    "notification": {
        "title": "Code Server Deployment",
        "message": "Your default password to log in to your Code Server instance is \"toortoor\", which you can change in container environment variables."
    },
    "description": "Code on any device with a consistent development environment.",
    "website": "https://hub.docker.com/r/linuxserver/code-server",
    "command": "/bin/sh",
    "environment": {
        "PASSWORD": "toortoor"
    },
    "ports": [{
        "protocol": "tcp",
        "internalPort": 8080
    }],
    "image": {
        "name": "codercom/code-server",
        "tag": "latest"
    }
}, {
    "name": "Wordpress",
    "husbands": [{
        "name": "Wordpress-DB",
        "command": "/bin/sh",
        "image": {
            "name": "mariadb",
            "tag": "latest"
        },
        "volumes": [{
            "containerPath": "/var/lib/mysql",
            "mode": "rw"
        }],
        "ports": [{
            "protocol": "tcp",
            "internalPort": 3306
        }],
        "environment": {
            "MARIADB_PASSWORD": "secret",
            "MARIADB_DATABASE": "wordpress",
            "MARIADB_USER": "manager",
            "MARIADB_ROOT_PASSWORD": "secret"
        }
    }],
    "volumes": [{
        "containerPath": "/var/www/html",
        "mode": "rw"
    }],
    "command": "/bin/sh",
    "environment": {
        "WORDPRESS_DB_NAME": "wordpress",
        "WORDPRESS_DB_HOST": "husbands:Wordpress-DB:3306",
        "WORDPRESS_DB_USER": "manager",
        "WORDPRESS_DB_PASSWORD": "secret"
    },
    "ports": [{
        "protocol": "tcp",
        "internalPort": 80
    }],
    "description": "Everything you need to build and grow any websiteâ€”all in one place.",
    "website": "https://wordpress.com/",
    "image": {
        "name": "wordpress",
        "tag": "latest"
    }
}, {
    "command": "/bin/sh",
    "name": "NGINX",
    "website": "https://nginx.org/",
    "description": "An open source reverse proxy server for HTTP, HTTPS, SMTP, POP3, and IMAP protocols.",
    "environment": {
        "NGINX_PORT": "80",
        "NGINX_HOST": ""
    },
    "ports": [{
        "protocol": "tcp",
        "internalPort": 80
    }],
    "image": {
        "name": "nginx",
        "tag": "latest"
    }
}]